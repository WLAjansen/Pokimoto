<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F5F5"
    tools:context=".activity_loginpagina1">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Welkom terug"
        android:textColor="@color/colorSecondary"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.192"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.219" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="109dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Log hieronder in"
        android:textSize="14dp"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="187dp" />

    <Button
        android:id="@+id/button"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="277dp"
        android:layout_height="39dp"
        android:layout_marginEnd="64dp"
        android:layout_marginRight="64dp"
        android:background="@drawable/custom_button_volgende"
        android:text="Account aanmaken"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="660dp" />

    <Button
        android:id="@+id/button"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="277dp"
        android:layout_height="39dp"
        android:layout_marginEnd="64dp"
        android:layout_marginRight="64dp"
        android:background="@drawable/custom_button_secondary"
        android:text="Inloggen"
        android:textAllCaps="false"
        android:textColor="#0066FF"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="543dp" />

    <EditText
        android:id="@+id/phone"
        android:hint="Email"
        android:layout_width="298dp"
        android:layout_height="46dp"
        android:background="@drawable/custom_input_primary"
        android:inputType="phone"
        tools:layout_editor_absoluteX="49dp"
        android:paddingLeft="20dp"
        tools:layout_editor_absoluteY="254dp"
        android:radius="10dp"/>



    <EditText
        android:id="@+id/phone"
        android:layout_width="298dp"
        android:layout_height="46dp"
        android:background="@drawable/custom_input_primary"
        android:hint="Wachtwoord"
        android:inputType="phone"
        android:paddingLeft="20dp"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="328dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="309dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Wachtwoord vergeten?"
        android:textSize="14dp"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="591dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="color/transparent"
        android:clickable="true"
        app:borderWidth="0dp"
        app:rippleColor="#8A6B6B"
        app:srcCompat="@drawable/abc_vector_test"
        app:useCompatPadding="false"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="16dp" />

</androidx.constraintlayout.widget.ConstraintLayout>